{
  "plugins": [
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "prettier",
    "airbnb"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "import/extensions": 0,
    "no-undef": 0,
    "linebreak-style": 0,
    "no-new": 0,
    "no-alert": "off",
    "no-console": "off",
    "no-extra-boolean-cast": 0,
    "operator-linebreak": ["error", "before"]
  }
}
